    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/compiler-sfc": "3.4.34",
    "@vue/runtime-dom": "^3.4.34",
    "chromatic": "^5.7.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
